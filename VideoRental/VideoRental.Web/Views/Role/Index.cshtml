@model IEnumerable<VideoRental.ViewModel.Views.RoleView>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Role List</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
    </tr>
}

</table>
